<?xml version="1.0" encoding="UTF-8"?>
<web-app version="4.0" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd">
    <filter>
        <filter-name>Filter</filter-name>
        <filter-class>com.mycompany.m1_sabrinatortelli.FilterM1</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>Filter</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>
    <servlet>
        <servlet-name>servletM1</servlet-name>
        <servlet-class>com.mycompany.m1_sabrinatortelli.servletM1</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>servletM1</servlet-name>
        <url-pattern>/servletM1</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
